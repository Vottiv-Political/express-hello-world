{
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "no-console": false,
    "object-literal-shorthand": false,
    "no-string-literal": false,
    "no-empty": false,
    "no-var-requires": false,
    "no-trailing-whitespace": false,
    "no-conditional-assignment": false,
    "variable-name": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["jobs/*.ts"]
  }
}
