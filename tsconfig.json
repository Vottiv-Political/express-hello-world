{
  "compilerOptions": {
    "module": "CommonJS",
    "strict": false,
    "esModuleInterop": true,
    "target": "ESNext",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "typeRoots": ["@types", "./node_modules/@types"],
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["*.ts"],
  "exclude": []
}
